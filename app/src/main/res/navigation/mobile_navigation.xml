<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.muramsyah.seakidul.ui.menu.home.MapsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_simulation"
        android:name="com.muramsyah.seakidul.ui.menu.simulation.SimulationFragment"
        android:label="@string/title_simulation"
        tools:layout="@layout/fragment_simulation" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.muramsyah.seakidul.ui.menu.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_setting" />
</navigation>